def is_palindrome(input_str):
    # 입력 문자열에서 공백 및 대소문자 구분 제거
    processed_str = ''.join(input_str.split()).lower()

    # 스택 초기화
    stack = []

    # 반복문을 통해 문자열의 절반을 스택에 넣음
    for char in processed_str[:len(processed_str)//2]:
        stack.append(char)

    # 문자열의 길이가 홀수인 경우 중간 문자는 무시
    if len(processed_str) % 2 == 1:
        processed_str = processed_str[len(processed_str)//2+1:]
    else:
        processed_str = processed_str[len(processed_str)//2:]

    # 스택에서 문자를 pop하며 남은 부분과 비교
    for char in processed_str:
        if char != stack.pop():
            return False

    return True

# 테스트
input_string = "dudud"
input_string2 = "hiimfall"
result = is_palindrome(input_string)
result2 = is_palindrome(input_string2)

if result:
    print(f'"{input_string}"은(는) 회문입니다.')
else:
    print(f'"{input_string}"은(는) 회문이 아닙니다.')

if result2:
    print(f'"{input_string2}"은(는) 회문입니다.')
else:
    print(f'"{input_string2}"은(는) 회문이 아닙니다.')
